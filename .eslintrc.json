{
  "env": {
      "es2021": true,
      "node": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
      "ecmaVersion": "latest"
  },
  "rules": {
      "no-console": "off",
      "camelcase": ["error", {"properties": "never", "ignoreDestructuring": true}],
      "no-underscore-dangle":  ["error", { "allow": ["_exports"], "allowAfterThis": true }]
  }
}
